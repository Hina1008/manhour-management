{
	"name": "Man-Hour Management",
	"manifest_version": 3,
	"version": "1.0",
	"permissions": [
		"activeTab",
		"scripting",
		"contextMenus",
		"storage",
		"notifications",
		"alarms"
	],
	"background": {
		"service_worker": "background.js"
	},
	"icons" : {
		"16" : "img/icon_16.png",
		"48" : "img/icon_48.png",
		"128" : "img/icon_128.png"
	  },
	"action": {
		"default_icon": {
			"16": "img/icon_16.png",
			"32": "img/icon_32.png",
			"48": "img/icon_48.png",
			"64": "img/icon_64.png",
			"128": "img/icon_128.png",
			"256": "img/icon_256.png"
		},
		"default_popup": "popup.html"
	}
}